'.platform-win32 atom-text-editor, .platform-linux atom-text-editor':
  'ctrl-d': 'editor:delete-line'
'atom-text-editor':
  'ctrl-d': 'editor:delete-line'

'atom-text-editor':
  'ctrl-alt-t': 'editor:auto-indent'
  'ctrl-shift-down': 'editor:add-selection-below'
  'ctrl-shift-up': 'editor:add-selection-above'
